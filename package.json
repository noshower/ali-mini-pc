{
  "name": "ali-app-pc",
  "displayName": "ali-app-pc",
  "description": "darcy ali mini app plugin",
  "version": "0.0.5",
  "publisher": "noshower",
  "repository": {
    "type": "git",
    "url": "https://github.com/noshower/ali-mini-pc.git"
  },
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "image/icon.png",
  "activationEvents": [
    "onCommand:darcy.createPage",
    "onCommand:darcy.createComponent",
    "onCommand:darcy.checkComponentName"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "darcy.createPage",
        "title": "新建小程序页面",
        "category": "darcy"
      },
      {
        "command": "darcy.createComponent",
        "title": "新建小程序组件",
        "category": "darcy"
      },
      {
        "command": "darcy.checkComponentName",
        "title": "检查小程序组件名",
        "category": "darcy"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "darcy.createComponent",
          "group": "darcy",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "darcy.createPage",
          "group": "darcy",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "darcy.checkComponentName",
          "group": "darcy",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.1",
    "fs-extra": "^9.0.1"
  },
  "license": "MIT"
}
